<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
<head>
<title>Uga Agga - Das Spiel</title>
<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1">
<link href="templates/css/style000.css" rel="stylesheet" type="text/css">
</head>
<body>
<table>
  <tr>
    <tmpl:SELECTOR>
      <td NOWRAP>
        <form name="form_{modus}" method="GET">
          <input type="hidden" name="modus" value="{modus}">
          <select name="{modus}ID" onChange="document.form_{modus}.submit()">
            <tmpl:OPTION>
              <option value="{value}" {selected}>{description}</option>
            </tmpl:OPTION>
          </select>
          <input type="submit" name="Submit" value="&gt;">
        </form>
      </td>
    </tmpl:SELECTOR>
  </tr>
</table>

<table width="100%" border="0" cellspacing="5" cellpadding="0">
  <tr>
    <td width="178" valign="top">
      <ul>
        <tmpl:MENU><li>&nbsp;<a href="{link}"<tmpl:TARGET> target="{target}"</tmpl:TARGET>>{content}</a></li></tmpl:MENU>
      </ul>
    </td>
    <td rowspan="2" valign="top">{content}</td>
  </tr>
  <tr>
    <td width="178" valign="top">
      <a href="http://www.uga-agga.de"><img src="templates/images/logo.gif" width="178" height="496" alt="www.uga-agga.de" title="www.uga-agga.de"></a>
    </td>
  </tr>
</table>

</body>
</html>
