    <script type="text/javascript">
      function serverTime() { 
        var time = null; 
        time = new Date("{{ countdown_time }}"); 
        return time; 
      }

      {% if jabber.server and jabber.passwd %}
        jQuery(document).ready(function() {
          DEVELOPER = 'on';

          HOST_MAIN      = '{{ jabber.server }}';
          HOST_MUC       = "conference.{{ jabber.server }}";
          HOST_PUBSUB    = "pubsub.{{ jabber.server }}";
          HOST_VJUD      = "vjud.{{ jabber.server }}";
          HOST_ANONYMOUS = "anonymous.{{ jabber.server }}";
          HOST_BOSH      = "http://{{ jabber.server }}:5280/http-bind/";
  
          JAPPIX_STATIC = ''

          // Define groupchats here
          MINI_GROUPCHATS = [{{ jabber.groupchats|raw }}];
          MINI_PASSWORDS = [];
          MINI_5D_NON_CLOSEABLE_GROUPCHATS = [{{ jabber.noClose|raw }}];
  
          // Define chats here
          MINI_CHATS = [];
  
          // Add an animation
          MINI_ANIMATE = false;
  
          // Define the user nickname
          MINI_NICKNAME = "{{ jabber.user }}";
  
          // Random user nickname (if no nickname)
          MINI_RANDNICK = false;
  
          // Override the default session resource
          MINI_RESOURCE = "UA-Game"+ Math.floor(Math.random()*100000);
  
          // Connect the user (autoconnect, show_pane, domain, username, password)
          // Notice: put true/false to autoconnect and show_pane
          // Notice: exclude "user" and "password" if using anonymous login
          launchMini(true, true, "{{ jabber.server }}", "{{ jabber.user|lower }}", "{{ jabber.passwd }}");
        });
      {% endif %}
    </script>
