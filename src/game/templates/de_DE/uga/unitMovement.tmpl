                  </div>
                </li>
                {% for item in selectable_movements %}
                  <li>
                    <div class="ym-fbox-check">
                      <input type="radio" name="movementID" value="{{ item.id }}" id="movementID{{ item.id }}" onfocus="dur();" onclick="dur();" onchange="dur();" onkeydown="dur();" onkeyup="dur();" />
