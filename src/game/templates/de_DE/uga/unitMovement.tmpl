              {% if item.artefact %}
                <li><span class="text-small" style="white-space: nowrap;">&nbsp;{{ item.artefact.name }}</span></li>
              {% endif %}
              </ul>
            </td>
            <td>&nbsp;</td> 
          </tr>
        {% endfor %}
        {% set bgID = 0 %}
      </tbody>
    </table>
  </div>
</div>
{% endif %}

{% endblock %}